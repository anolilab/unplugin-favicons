        "@nuxt/kit": "^3.14.1592",
        "@nuxt/schema": "^3.14.1592",
        "@rspack/core": "^1.1.6",
        "@rspack/plugin-html": "^0.5.8",
        "@secretlint/secretlint-rule-preset-recommend": "^9.0.0",
        "@sveltejs/kit": "^2.12.2",
        "@total-typescript/ts-reset": "^0.6.1",
        "@tsconfig/esm": "^1.0.5",
