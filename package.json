{
    "name": "@anolilab/unplugin-favicons",
    "version": "1.0.5-alpha.1",
    "description": "Generate favicons for your project with caching for blazing fast rebuilds.",
    "keywords": [
        "plugin",
        "pwa",
        "vite-plugin",
        "vite",
        "favicon",
        "favicons",
        "image",
        "generate",
        "webpack",
        "rspack",
        "rollup",
        "astro",
        "esbuild",
        "nuxt",
        "sveltekit",
        "svelte",
        "generating-favicons",
        "favicons-generator"
    ],
    "homepage": "https://anolilab.com/nodejs/packages/unplugin-favicons",
    "bugs": {
        "url": "https://github.com/anolilab/unplugin-favicons/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/anolilab/unplugin-favicons.git"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de",
        "url": "https://danielbannert.com"
    },
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.mts",
            "default": "./dist/index.mjs"
        },
        "./vite": {
            "types": "./dist/vite.d.mts",
            "default": "./dist/vite.mjs"
        },
        "./webpack": {
            "types": "./dist/webpack.d.mts",
            "default": "./dist/webpack.mjs"
        },
        "./rollup": {
            "types": "./dist/rollup.d.mts",
            "default": "./dist/rollup.mjs"
        },
        "./esbuild": {
            "types": "./dist/esbuild.d.mts",
            "default": "./dist/esbuild.mjs"
        },
        "./nuxt": {
            "types": "./dist/nuxt.d.mts",
            "default": "./dist/nuxt.mjs"
        },
        "./package.json": "./package.json"
    },
    "module": "dist/index.mjs",
    "types": "dist/index.d.mts",
    "files": [
        "dist/**",
        "CHANGELOG.md",
        "LICENSE.md",
        "SECURITY.md",
        "README.md"
    ],
    "scripts": {
        "audit": "audit-ci --config ./audit-ci.jsonc",
        "build": "cross-env NODE_ENV=development packem build",
        "build:prod": "cross-env NODE_ENV=production packem build",
        "lint:eslint": "eslint . --ext js,cjs,mjs,jsx,ts,tsx,json,yaml,yml,md,mdx --max-warnings=0 --config .eslintrc.cjs",
        "lint:eslint:fix": "pnpm run lint:eslint --fix",
        "lint:package-json": "publint",
        "lint:prettier": "prettier --config=.prettierrc.cjs --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.cjs --write .",
        "lint:secrets": "secretlint **/*",
        "lint:staged": "lint-staged --verbose --concurrent false --debug",
        "lint:text": "textlint ./.github/ ./README.md ./UPGRADE.md --parallel --experimental --cache --dry-run",
        "lint:text:fix": "textlint ./.github/ ./README.md ./UPGRADE.md --parallel --experimental --fix",
        "lint:types": "tsc --noEmit",
        "prepare": "is-ci || (node verify-node-version.cjs && pnpx only-allow pnpm && husky)",
        "run:example": "cd example && pnpm exec vite build",
        "sort-package-json": "sort-package-json ./package.json examples/**/package.json",
        "test": "vitest run",
        "test:bench": "vitest bench",
        "test:coverage": "vitest run --coverage",
        "test:watch": "vitest",
        "update:deps": "taze"
    },
    "dependencies": {
        "@visulima/find-cache-dir": "^1.0.6",
        "@visulima/package": "^3.0.7",
        "cacache": "^18.0.4",
        "consola": "3.2.3",
        "fast-json-stable-stringify": "^2.1.0",
        "parse-author": "^2.0.0",
        "parse5": "^7.1.2",
        "ssri": "^10.0.6",
        "unplugin": "^1.12.2"
    },
    "devDependencies": {
        "@anolilab/commitlint-config": "^5.0.3",
        "@anolilab/eslint-config": "^15.0.3",
        "@anolilab/lint-staged-config": "^2.1.7",
        "@anolilab/prettier-config": "^5.0.14",
        "@anolilab/semantic-release-preset": "^9.0.0",
        "@anolilab/textlint-config": "^8.0.16",
        "@babel/core": "^7.25.2",
        "@babel/eslint-parser": "7.25.1",
        "@commitlint/cli": "^19.4.0",
        "@commitlint/config-conventional": "^19.2.2",
        "@nuxt/kit": "^3.12.4",
        "@nuxt/schema": "^3.12.4",
        "@rspack/core": "^0.7.5",
        "@rspack/plugin-html": "^0.5.8",
        "@secretlint/secretlint-rule-preset-recommend": "^8.2.4",
        "@total-typescript/ts-reset": "^0.5.1",
        "@tsconfig/esm": "^1.0.5",
        "@tsconfig/node18": "^18.2.4",
        "@tsconfig/strictest": "^2.0.5",
        "@types/cacache": "^17.0.2",
        "@types/node": "^22.1.0",
        "@types/parse-author": "^2.0.3",
        "@types/parse5": "^7.0.0",
        "@types/ssri": "^7.1.5",
        "@visulima/packem": "^1.0.0-alpha.81",
        "@vitest/coverage-v8": "^2.0.5",
        "audit-ci": "^7.1.0",
        "caniuse-lite": "^1.0.30001651",
        "commitizen": "^4.3.0",
        "commitlint": "^19.4.0",
        "conventional-changelog-conventionalcommits": "8.0.0",
        "cross-env": "^7.0.3",
        "cz-conventional-changelog": "^3.3.0",
        "esbuild": "^0.23.1",
        "eslint": "8.57.0",
        "eslint-plugin-deprecation": "^3.0.0",
        "eslint-plugin-editorconfig": "^4.0.3",
        "eslint-plugin-import": "npm:eslint-plugin-i@2.29.1",
        "eslint-plugin-mdx": "^3.1.5",
        "eslint-plugin-n": "^17.10.2",
        "eslint-plugin-vitest": "0.4.1",
        "eslint-plugin-vitest-globals": "^1.5.0",
        "favicons": "^7.2.0",
        "html-webpack-plugin": "^5.6.0",
        "husky": "^9.1.5",
        "is-ci": "^3.0.1",
        "lint-staged": "^15.2.9",
        "pkg-pr-new": "^0.0.20",
        "prettier": "^3.3.3",
        "publint": "^0.2.10",
        "read-pkg": "9.0.1",
        "rimraf": "^6.0.1",
        "rollup": "^4.21.0",
        "secretlint": "8.2.4",
        "semantic-release": "^24.1.0",
        "sort-package-json": "^2.10.0",
        "taze": "^0.16.6",
        "textlint": "^14.2.0",
        "type-fest": "^4.25.0",
        "typescript": "^5.5.4",
        "vite": "^5.4.2",
        "vitest": "^2.0.5",
        "webpack": "^5.93.0"
    },
    "peerDependencies": {
        "@nuxt/kit": "^3",
        "@nuxt/schema": "^3",
        "@rspack/cli": "^0.3.6",
        "esbuild": "*",
        "favicons": "^7.0.1",
        "rollup": "^3",
        "vite": "^3 || ^4 || ^5",
        "webpack": "^4 || ^5"
    },
    "peerDependenciesMeta": {
        "@nuxt/kit": {
            "optional": true
        },
        "@nuxt/schema": {
            "optional": true
        },
        "@rspack/cli": {
            "optional": true
        },
        "esbuild": {
            "optional": true
        },
        "rollup": {
            "optional": true
        },
        "vite": {
            "optional": true
        },
        "webpack": {
            "optional": true
        }
    },
    "packageManager": "pnpm@9.7.1",
    "engines": {
        "node": ">=18.0.0"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "pnpm": {
        "overrides": {
            "@babel/traverse@<7.23.2": ">=7.25.3",
            "braces@<3.0.3": ">=3.0.3",
            "chrono-node@<2.2.4": ">=2.2.4",
            "cosmiconfig-typescript-loader": "^5.0.0",
            "follow-redirects@<1.15.4": ">=1.15.6",
            "follow-redirects@<=1.15.5": ">=1.15.6",
            "got@<11.8.5": ">=14.4.2",
            "load-tsconfig": "^0.2.5",
            "tar@<6.2.1": ">=7.4.3",
            "trim@<0.0.3": ">=1.0.1",
            "tsconfig-paths": "^4.2.0",
            "webpack-dev-middleware@<=5.3.3": ">=7.4.2",
            "ws@>=8.0.0 <8.17.1": ">=8.17.1",
            "ws@>=7.0.0 <7.5.10": ">=7.5.10",
            "zod@<=3.22.2": ">=3.23.8"
        }
    },
    "anolilab": {
        "eslint-config": {
            "plugin": {
                "tsdoc": false,
                "etc": false
            },
            "warn_on_unsupported_typescript_version": false,
            "info_on_disabling_jsx_react_rule": false,
            "info_on_disabling_prettier_conflict_rule": false,
            "info_on_disabling_jsonc_sort_keys_rule": false
        },
        "lint-staged-config": {
            "typescript": {
                "exclude": [
                    "examples/sveltekit"
                ]
            }
        }
    }
}
